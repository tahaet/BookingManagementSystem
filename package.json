{
  "name": "starter",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start:prod": "set NODE_ENV=production && concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "copy-views": "copyfiles -u 1 src/views/**/*.pug dist/",
    "copy-public": "copyfiles -u 1 src/public/**/* dist/",
    "watch-views": "nodemon --watch src/views -e pug --exec \"npm run copy-views\"",
    "watch-public": "nodemon --watch src/public -e pug --exec \"npm run copy-public\"",
    "watch:js": "parcel watch ./src/public/js/index.js --out-dir ./dist/public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./dist/public/js --out-file bundle.js",
    "start": "concurrently \"tsc --watch\" \"npm run watch:js\" \"npm run watch-views\" \"npm run watch-public\" \"nodemon dist/server.js\"",
    "debug": "ndb server.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@express-rate-limit/tsconfig": "^1.0.2",
